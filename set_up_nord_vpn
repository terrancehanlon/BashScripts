!#/bin/bash

sudo apt-get install openvpn;

cd /etc/openvpn;

sudo wget https://downloads.nordcdn.com/configs/archives/servers/ovpn.zip;

sudo apt-get install ca-certificates;

echo "switch to /etc/openvpn";

cd /;
cd etc;
cd openvpn;

ls;

sudo apt-get install unzip;
sudo unzip ovpn.zip;
sudo rm zip;


